{
  "name": "jssp",
  "displayName": "JSSP",
  "description": "Syntax highlighting support for JSSP (JavaScript Server Pages)",
  "version": "0.0.1",
  "publisher": "CTRL",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisTreb/jssp.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jssp",
        "aliases": [
          "JSSP",
          "jssp"
        ],
        "extensions": [
          ".jssp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jssp",
        "scopeName": "source.jssp",
        "path": "./syntaxes/jssp.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "jssp",
        "path": "./snippets/jssp.json"
      }
    ]
  }
}