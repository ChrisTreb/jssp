{
    "JSSP For Loop": {
        "prefix": "jsspfor",
        "body": [
            "<% for (let ${1:i} = 0; ${1:i} < ${2:10}; ${1:i}++) { %>",
            "  ${3:// code here}",
            "<% } %>"
        ],
        "description": "Boucle for en JSSP"
    },
    "JSSP For Each Loop with Custom Function": {
        "prefix": "jsspforeachfunc",
        "body": [
        "<% for each (var ${1:item} in ${2:data}) { %>",
        "  <div>${1:item}</div>",
        "<% } %>"
        ],
        "description": "Boucle for each en JSS"
    },
    "JSSP If Statement": {
        "prefix": "jsspif",
        "body": [
            "<% if (${1:condition}) { %>",
            "  ${2:// code here}",
            "<% } %>"
        ],
        "description": "Condition if en JSSP"
    },
    "JSSP use variable": {
        "prefix": "jsspvar",
        "body": [
            "<%= ${1:foo} %>"
        ],
        "description": "Utiliser une variable en JSSP"
    }
}