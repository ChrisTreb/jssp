{
    "JSSP For Loop": {
        "prefix": "forjssp",
        "body": [
            "<% for (let ${1:i} = 0; ${1:i} < ${2:10}; ${1:i}++) { %>",
            "  ${3:// code here}",
            "<% } %>"
